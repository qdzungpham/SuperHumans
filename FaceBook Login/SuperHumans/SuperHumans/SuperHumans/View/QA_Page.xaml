<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SuperHumans.View.QA_Page"
             Title="" Icon="logoo" BackgroundColor="White">

    <ContentPage.ToolbarItems>
        <ToolbarItem Text=""  Priority="1" Order="Primary" Icon="homeg"/>
        <ToolbarItem Text="" Priority="1" Order="Primary"  Icon="glass"/>
        <ToolbarItem Text="" Priority="1" Order="Primary"  Icon="lippp"/>
        <ToolbarItem Text="" Priority="1" Order="Secondary"  Icon="lipreal"/>
    </ContentPage.ToolbarItems>

    <StackLayout>

        <Button Text=""
          
          HorizontalOptions="Center"
          VerticalOptions="Center"
          
          Image ="ask.png"      
               
          Clicked="GoAskQuestionButton"/>


        <Button Text=""
            HorizontalOptions="Center"
            VerticalOptions="Center"
            Image ="browse.png"       
            Clicked="GoBrowseQButton"/>

        <Button Text="PREVIOUS PAGE" BackgroundColor="Orange" TextColor="White"  HeightRequest="70" FontSize="Medium" FontAttributes="Bold" Clicked="PreviousPageButton"/>
        <!--Command="{Binding QuestionListCommand}}"-->

    </StackLayout>

</ContentPage>