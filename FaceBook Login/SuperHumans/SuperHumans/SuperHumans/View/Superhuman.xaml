<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SuperHumans.View.Superhuman"
             Title="Superhuman">

    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Home"  Priority="1" Order="Primary"/>
        <ToolbarItem Text="Speak" Priority="1" Order="Secondary"/>
    </ContentPage.ToolbarItems>

    <StackLayout>

        <Button Text="Browse SuperHumans" Clicked="BrowseSuperHumanButton"/>

        <Button Text="Register A SuperHuman" />

        <Button Text="PREVIOUS PAGE" BackgroundColor="Silver" FontAttributes="Bold" TextColor="Blue" Clicked="PreviousPageButton"/>
    </StackLayout>
    


</ContentPage>